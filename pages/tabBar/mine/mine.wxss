/* pages/tabBar/mine/mine.wxss */
.sign-icon{
  position:absolute;
  width:90rpx;
  color:#fff;
  font-size:30rpx;
  text-align: center;
  background-color:#f9cf12;
  padding: 10rpx 10rpx;
  border-radius: 5rpx;
  top:30rpx;
  right:30rpx;
  box-shadow: 0 0 1rpx #f8cc00;
}
.player-detail{
  margin-left:20rpx;
  margin-top:50rpx;
}
.me-head{
  margin-left:40rpx;
  vertical-align: middle;
  margin-top:50rpx;
}
.band-phone{
  color:#6e68f6;
  font-size:24rpx;
  letter-spacing: 2rpx;
  margin-top:20rpx;
}
.baby-head{
  margin-left:40rpx;
  vertical-align: middle;
  margin-top:50rpx;
}
.baby-detail{
  margin-top:30rpx;
  margin-left:20rpx;
}
/* 红包竞猜币 */
.packet-wrapper{
  width:100%;
  text-align: center;
}
.packet-quize{
  display:inline-flex;
  width:95%;
  background-color:#fff;
  border-radius: 12rpx;
  flex-direction: row;
  justify-content: space-around;
  margin-top:110rpx;
  box-shadow: 0 0 10rpx #dbdbdb;
  align-items: center;
  padding:30rpx 0 10rpx 0;
}
.packet-quize image{
  width:50rpx;
  height:50rpx;
  vertical-align: middle;
}
.packet-quize .dis-num{
  color:#333;
  font-size:32rpx;
}
.packet-quize .img-wraper{
  color:#929292;
  font-size:26rpx;
  margin-top:10rpx;
}
.packet-quize .line{
  height:80rpx;
  width:2rpx;
  background-color:#929292;
}
.cheer{
  width:95%;
  background-color:#fff;
  margin:0 auto;
  border-radius: 12rpx;
  font-size:30rpx;
  color:#5e5e5e;
  margin-top:20rpx;
  height:110rpx;
  line-height: 110rpx;
  box-shadow: 0 0 10rpx #dbdbdb;
}
.cheer text{
  display:inline-block;
}
.cheer view{
  float:right;
  margin-right:20rpx;
  color:#6e68f6;
  font-size:27rpx;
}
.cheer image{
  width:30rpx;
  height:30rpx;
  vertical-align: middle;
  margin-left:10rpx;
}
.cheer text:first-child{
  margin-left:25rpx;
}
.cheer text:nth-child(2){
  margin-left:50rpx;
}
.baby-page-head-words{
  margin-left:40rpx !important;
  font-size:30rpx !important;
  color:#5e5e5e;
}
.baby-album{
  width:100%;
  height:250rpx;
  text-align: center;
  background-color:#fff;
}
.baby-album .image-wrapper{
  display:inline-block;
  width:220rpx;
  height:220rpx;
  overflow: hidden;
}
.baby-album .image-wrapper:nth-child(2),.baby-album .image-wrapper:nth-child(3){
  margin-left:10rpx;
}
.baby-album .image-wrapper image{
  height:100%;
}
.shadow-lighter{
  background-color:#fff;
  height:90rpx;
  line-height: 90rpx;
  margin-top:20rpx;
}
.shadow-lighter image{
  width:30rpx;
  height:30rpx;
  vertical-align: middle;
}
.shadow-lighter .more-match{
  display:block;
  font-size:27rpx;
  float:right;
  margin-right:40rpx;
  color:#929292;
}
/* 底部公共信息 */
.bottom-block{
  width:95%;
  background-color:#fff;
  border-radius: 12rpx;
  margin:0 auto;
  padding-top:5rpx;
  margin-top:20rpx;
  font-size:30rpx;
  color:#5e5e5e;
}
.bottom-block view{
  display:inline-block;
}
.bottom-block image{
  width:30rpx;
  height:30rpx;
  vertical-align: middle;
}
.bottom-block .block-one{
  height:100rpx;
  line-height: 100rpx;
  width:100%;
  border-bottom: 1rpx solid #ececec;
}
.block-one .head-words{
  display:inline-block;
  margin-left:30rpx;
}
.bottom-block  .more-match{
  float:right;
  margin-right:20rpx;
  color:#929292;
  font-size:27rpx;
}
.bottom{
  width:100%;
  height:80rpx;
}

